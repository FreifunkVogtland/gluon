+		tplink_pharos_check_image "$1" "7f454c46" && return 0
+		return 1
+		;;
+	cpe210v2)
+		# magic = mktplinkfw v1 header
+		tplink_pharos_check_image "$1" "01000000" && return 0
 		return 1
 		;;
 	a40|\
