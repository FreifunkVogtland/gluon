index fcf05dbda152ae397fdad7f965f559b415c7c934..2772b1cc40aaac08a3a9bd27bf94b9503eea9705 100644
--- a/batctl/Makefile
+++ b/batctl/Makefile
@@ -10,7 +10,7 @@ include $(TOPDIR)/rules.mk
